# Environment configuration for Flash Loan Arbitrage Bot
# Using environment variables loaded from secrets

# BNB Chain RPC URLs
BNB_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# Keys for deployment and authentication
PRIVATE_KEY=${PRIVATE_KEY}
BNB_RPC_URL_KEY=${BNB_RPC_URL_KEY}
SESSION_SECRET=${SESSION_SECRET}

# Bot configuration
USE_REAL_BLOCKCHAIN=true
PRODUCTION_MODE=true
ENABLE_DASHBOARD=true